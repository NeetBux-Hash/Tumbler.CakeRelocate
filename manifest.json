{
	"$schema": "https://raw.githubusercontent.com/atampy25/simple-mod-framework/main/Mod%20Manager/src/lib/manifest-schema.json",
	"version": "0.6.0",
	"id": "Tumbler.CakeTopper",
	"name": "ReloCake",
	"description": "A truly amazing cake (re)moving mod",
	"authors": ["Tumbler"],
	"frameworkVersion": "2.33.20",
	"contentFolders": [],
	"options": [
		{
			"group": "The Cake at Hand",
			"name": "Remove the Cake",
			"type": "select",
			"tooltip": "Deletes every trace of its existence",
			"contentFolders": ["Options/delete"]
		},
		{
			"group": "The Cake at Hand",
			"name": "ReloCake it",
			"type": "select",
			"tooltip": "Moves the cake slightly to the side instead",
			"enabledByDefault": true,
			"contentFolders": ["Options/move"]
		},
		{
			"group": "The Cake at Hand",
			"name": "Test",
			"type": "select",
			"tooltip": "Moves the cake less noticeably to the side. Not sure if it's intel safe compatible. Feel free to confirm on nexus :)",
			"enabledByDefault": false,
			"contentFolders": ["Options/test"]
		}

	],
	"updateCheck": "https://github.com/NeetBux-Hash/Tumbler.CakeRelocate/releases/latest/download/updates.json"
}
